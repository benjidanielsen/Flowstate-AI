{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start Flowstate-AI System (Windows)",
            "type": "shell",
            "command": "start /B python \"\MANUS_SYNC_ENGINE_ENHANCED.py\" && start /B gunicorn -w 4 -b 0.0.0.0:3333 \"\godmode-dashboard\app_enhanced:app\"",
            "windows": {
                "command": "start /B python \"\MANUS_SYNC_ENGINE_ENHANCED.py\" && start /B gunicorn -w 4 -b 0.0.0.0:3333 \"\godmode-dashboard\app_enhanced:app\""
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "problemMatcher": []
        },
        {
            "label": "Stop Flowstate-AI System (Windows)",
            "type": "shell",
            "command": "taskkill /IM python.exe /F || true && taskkill /IM gunicorn.exe /F || true",
            "windows": {
                "command": "taskkill /IM python.exe /F || true && taskkill /IM gunicorn.exe /F || true"
            },
            "group": "none",
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "problemMatcher": []
        }
    ]
}
